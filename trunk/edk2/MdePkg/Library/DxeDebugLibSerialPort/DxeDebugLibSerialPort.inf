################################################################################
[Defines]
  INF_VERSION                    = 0x00010005
  BASE_NAME                      = DxeDebugLibSerialPort
  FILE_GUID                      = BB83F95F-EDBC-4884-A520-CD42AF388FAE
  MODULE_TYPE                    = DXE_DRIVER
  VERSION_STRING                 = 1.0
  LIBRARY_CLASS                  = DebugLib 
